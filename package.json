{
  "name": "crm-app",
  "version": "1.0.2",
  "description": "It is an app developed for resolving customer queries using backend libraries viz. express, bcrypt, jsonwebtoken, dotenv, mongoose, etc.",
  "main": "app.js",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --testEnvironment=node --runInBand --detectOpenHandles --coverage ./tests",
    "build": "npm install axios bcrypt cors dotenv express jest jsonwebtoken mongodb-memory-server mongoose morgan validator express-rate-limit --save",
    "start": "node app.js",
    "dev": "nodemon app.js",
    "lint:fix": "eslint --fix ./*.js"
  },
  "keywords": [
    "nodejs",
    "bcrypt",
    "express",
    "dotenv",
    "jsonwebtoken",
    "mongoose",
    "nodemon",
    "morgan",
    "cors",
    "express-rate-limit",
    "validator"
  ],
  "author": "Nilanjan Haldar",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.3",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb-memory-server": "^8.15.1",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nil2022/CRM_App.git"
  },
  "bugs": {
    "url": "https://github.com/nil2022/CRM_App/issues"
  },
  "homepage": "https://github.com/nil2022/CRM_App#readme"
}
